<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="workreportns">

	<select id="getWorkReportListAll" resultType="workreport">
		select * from WorkReport order by idx desc
	</select>
	
	<select id="" parameterType="integer">
		select * from WorkReportDetail where idx = ${idx}  
	</select>

</mapper>